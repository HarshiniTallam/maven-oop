package com.epam2.oops;

public class Chocolates {
	int price, weight, name;

	public Chocolates(int price, int weight, int name) {
		super();
		this.price = price;
		this.weight = weight;
		this.name = name;
	}

	public int getPrice() {
		return price;
	}

	public void setPrice(int price) {
		this.price = price;
	}

	public int getWeight() {
		return weight;
	}

	public void setWeight(int weight) {
		this.weight = weight;
	}
	
}
