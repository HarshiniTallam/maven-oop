package com.epam2.oops;

public class Sweets {
	int price, weight, name;
	
	public Sweets(int price, int weight, int name) {
		super();
		this.price = price;
		this.weight = weight;
		this.name = name;
	}
	
	public int getPrice() {
		return price;
	}

	public void setPrice(int price) {
		this.price = price;
	}

	public int getName() {
		return name;
	}

	public void setName(int name) {
		this.name = name;
	}
	

}
